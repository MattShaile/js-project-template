{
  "name": "react-template",
  "version": "1.0.0",
  "description": "Boilerplate for full-stack js project using react/express - fork this to start",
  "scripts": {
    "lint": "eslint -c ./config/eslint.config.json ./src/js --ext jsx",
    "test": "mocha src/**/*.test.js --compilers js:babel-core/register --require ignore-styles",
    "webpack-dev": "webpack-dev-server --config ./config/webpack.config.js --host 0.0.0.0 --content-base public --inline --hot",
    "webpack-build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "dev": "npm run webpack-dev",
    "build": "npm run lint && npm run test && npm run webpack-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://Rustygames@github.com/Rustygames/react-template.git"
  },
  "keywords": [
    "react",
    "template"
  ],
  "author": "Matthew Shaile",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rustygames/react-template/issues"
  },
  "homepage": "https://github.com/Rustygames/react-template#readme",
  "dependencies": {
    "axios": "^0.12.0",
    "express": "^4.14.1",
    "history": "^1.17.0",
    "lodash": "^4.17.4",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-promise": "^0.5.3",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-template-literals": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "ignore-styles": "^5.0.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.3.0",
    "npm-run-all": "^4.0.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
